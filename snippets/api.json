{
  "api-head": {
    "prefix": "api.head",
    "body": [
      "{",
      "\tmethod: 'HEAD',",
      "\turl: '$1'",
      "}"
    ],
    "description": "Ping data (api-testing-tool)"
  },
  "api-get": {
    "prefix": "api.get",
    "body": [
      "{",
      "\tmethod: 'GET',",
      "\turl: '$1'",
      "}"
    ],
    "description": "GET data (api-testing-tool)"
  },
  "api-delete": {
    "prefix": "api.delete",
    "body": [
      "{",
      "\tmethod: 'DELETE',",
      "\turl: '$1'",
      "}"
    ],
    "description": "Delete data (api-testing-tool)"
  },
  "api-post-json": {
    "prefix": "api.post.json",
    "body": [
      "{",
        "\tmethod: 'POST',",
        "\turl: '$1',",
        "\trequestHeader: {",
          "\t\t'content-type': 'application/json'",
        "\t},",
        "\trequestBody: ${2:jsonObject}",
      "}"
    ],
    "description": "Add JSON data (api-testing-tool)"
  },
  "api-post-form": {
    "prefix": "api.post.form",
    "body": [
      "{",
        "\tmethod: 'POST',",
        "\turl: '$1',",
        "\trequestHeader: {",
          "\t\t'content-type': 'application/x-www-form-urlencoded'",
        "\t},",
        "\trequestBody: {",
          "\t\t${2:fieldName}: ${3:fieldValue}",
        "\t}",
      "}"
    ],
    "description": "Add form data key-value (api-testing-tool)"
  },
  "api-put-json": {
    "prefix": "api.put.json",
    "body": [
      "{",
        "\tmethod: 'PUT',",
        "\turl: '$1',",
        "\trequestHeader: {",
          "\t\t'content-type': 'application/json'",
        "\t},",
        "\trequestBody: ${2:jsonObject}",
      "}"
    ],
    "description": "Update JSON data (api-testing-tool)"
  },
  "api-put-form": {
    "prefix": "api.put.form",
    "body": [
      "{",
        "\tmethod: 'PUT',",
        "\turl: '$1',",
        "\trequestHeader: {",
          "\t\t'content-type': 'application/x-www-form-urlencoded'",
        "\t},",
        "\trequestBody: {",
          "\t\t${2:fieldName}: ${3:fieldValue}",
        "\t}",
      "}"
    ],
    "description": "Update form data key-value (api-testing-tool)"
  },
  "api-post-file": {
    "prefix": "api.post.file",
    "body": [
      "{",
        "\tmethod: 'POST',",
        "\turl: '$1',",
        "\trequestHeader: {",
          "\t\t'content-type': 'multipart/form-data'",
        "\t},",
        "\trequestBody: {",
          "\t\t'file:${2:fieldName}': '${3:fileAbsolutePath}'",
        "\t}",
      "}"
    ],
    "description": "Upload file. File can be string array or string which is absolute path to file.\nYou can add field without file too (api-testing-tool)"
  },
  "api-req-header": {
    "prefix": "api.req-header",
    "body": [
      "requestHeader: {",
        "\t$1: $2",
      "}"
    ],
    "description": "Add request header (api-testing-tool)"
  },
  "api-req-body": {
    "prefix": "api.req-body",
    "body": [
      "requestBody: {",
        "\t'$1': '$2'",
      "}"
    ],
    "description": "Add request body (api-testing-tool)"
  },
  "api-object": {
    "prefix": "api.object",
    "body": [
      "{$1: $2}"
    ],
    "description": "Create object"
  },
  "api-array": {
    "prefix": "api.array",
    "body": [
      "[$1]"
    ],
    "description": "Create array"
  }
}